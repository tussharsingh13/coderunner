<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.label :first_name %> &nbsp
    <%= f.text_field :first_name, autocomplete: "off" %>
  </div>
  <br/>
  <div class="field">
    <%= f.label :last_name %>&nbsp
    <%= f.text_field :last_name, autocomplete: "off" %>
  </div>
   <br/>
   <div class="field">
    <%= f.label :username %> &nbsp
    <%= f.text_field :username, autocomplete: "off" %>
  </div>
  <br/>
  <div class="field">
    <%= f.label :email %>&nbsp
    <%= f.email_field :email, autofocus: true %>
  </div>
 <br/>
  <div class="field">
    <%= f.label :password %>
    <% if @validatable %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %>&nbsp
    <%= f.password_field :password, autocomplete: "off" %>
  </div>
 <br/>
  <div class="field">
    <%= f.label :password_confirmation %>&nbsp
    <%= f.password_field :password_confirmation, autocomplete: "off" %>
  </div>
   <br/>
  <div class="field">
    <%= f.label :college %>&nbsp
    <%= f.text_field :college, autocomplete: "off" %>
  </div>
 <br/>
  <div class="field">
    <%= f.label :about_me %>&nbsp
    <%= f.text_area :about_me, autocomplete: "off" %>
  </div>
   <br/>
  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>
 <br/>
<%= render "devise/shared/links" %>
